		<group>
			<id>1</id>
		</group>
		<what>FacilityCycle</what>
	</rule>

	<rule>
		<crudFlags>CRUD</crudFlags>
		<group>
			<id>1</id>
		</group>
		<what>Investigation</what>
	</rule>

	<rule>
		<crudFlags>CRUD</crudFlags>
		<group>
			<id>1</id>
		</group>
		<what>Keyword</what>
	</rule>

	<rule>
		<crudFlags>CRUD</crudFlags>
		<group>
			<id>1</id>
		</group>
		<what>InvestigationUser</what>
	</rule>

	<rule>
