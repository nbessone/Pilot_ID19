		<group>
			<id>1</id>
		</group>
		<what>Sample</what>
	</rule>

	<rule>
		<crudFlags>CRUD</crudFlags>
		<group>
			<id>1</id>
		</group>
		<what>SampleParameter</what>
	</rule>

	<rule>
		<crudFlags>CRUD</crudFlags>
		<group>
